  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.16.3",
    "prom-client": "^15.1.2",
    "zod": "^4.1.12",
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.1.0",
    "long": "^5.2.3",
    "protobufjs": "^7.3.0"
  },
  "devDependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
