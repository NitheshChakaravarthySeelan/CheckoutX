    "@types/node": "^20.19.23",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "ts-proto": "^1.168.0",
    "grpc-tools": "^1.12.4"
  }
